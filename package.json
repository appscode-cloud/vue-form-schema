{
  "name": "@appscode/vue-openapi-form",
  "version": "0.1.0",
  "main": "dist/vue-openapi-form.ssr.js",
  "browser": "dist/vue-openapi-form.esm.js",
  "module": "dist/vue-openapi-form.esm.js",
  "unpkg": "dist/vue-openapi-form.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/App.vue",
    "!src/main.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "pkg": "rollup --config rollup.config.js",
    "pkg:ssr": "cross-env NODE_ENV=production rollup --config rollup.config.js --format cjs",
    "pkg:es": "cross-env NODE_ENV=production rollup --config rollup.config.js --format es",
    "pkg:unpkg": "cross-env NODE_ENV=production rollup --config rollup.config.js --format iife",
    "deploy": "firebase use default && firebase deploy"
  },
  "dependencies": {
    "@appscode/design-system": "^1.0.43-alpha.153",
    "@appscode/design-system-images": "0.0.13",
    "bulma": "^0.9.2",
    "bulma-switch": "^2.0.0",
    "core-js": "^3.8.3",
    "font-awesome": "^4.7.0",
    "js-yaml": "^3.14.1",
    "v-tooltip": "^3.0.0-beta.1",
    "vee-validate": "^3.3.9",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.16",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.13.5",
    "@babel/runtime": "^7.10.5",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^5.0.0",
    "@vue/cli-plugin-eslint": "^5.0.0",
    "@vue/cli-service": "^5.0.0",
    "babel-cli": "^6.26.0",
    "cross-env": "^6.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "minimist": "^1.2.5",
    "monaco-editor": "^0.33.0",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "path": "^0.12.7",
    "prettier": "^2.0.5",
    "rollup": "^2.23.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "^1.50.0",
    "sass-loader": "^10.1.1",
    "vue-template-compiler": "^2.6.14",
    "vuex": "^3.5.1"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=16"
  },
  "description": "A Vue component to generate html form using OpenAPI v3 schema",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appscode/vue-openapi-form.git"
  },
  "keywords": [
    "json-schema",
    "vue",
    "openapi",
    "kubernetes"
  ],
