{
  "name": "vue-form-schema",
  "version": "0.1.0",
  "main": "dist/vue-form-schema.umd.js",
  "module": "dist/vue-form-schema.esm.js",
  "unpkg": "dist/vue-form-schema.min.js",
  "browser": {
    "./sfc": "src/vue-form-schema.vue"
  },
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "pkg": "npm run pkg:umd & npm run pkg:es & npm run pkg:unpkg",
    "pkg:umd": "rollup --config pkg/rollup.config.js --format umd --file dist/vue-form-schema.umd.js",
    "pkg:es": "rollup --config pkg/rollup.config.js --format es --file dist/vue-form-schema.esm.js",
    "pkg:unpkg": "rollup --config pkg/rollup.config.js --format iife --file dist/vue-form-schema.min.js"
  },
  "dependencies": {
    "bulma": "^0.8.0",
    "bulma-switch": "^2.0.0",
    "core-js": "^3.3.2",
    "font-awesome": "^4.7.0",
    "vee-validate": "^3.1.3",
    "vue": "^2.6.10",
    "vue-codemirror": "^4.0.6",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "rollup-plugin-uglify-es": "0.0.1",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
