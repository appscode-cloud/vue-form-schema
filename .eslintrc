{
  "env": {
    "browser": true
  },
  "extends": [
    "plugin:vue/essential",
    "plugin:vue/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "vue",
    "html"
  ],
  "rules": {
    "vue/multi-word-component-names": "off"
  }
}